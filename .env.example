# Environment variables for Ethereum Reputation Passport

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/infura-key
KADENA_RPC_URL=https://api.testnet.chainweb.com

# API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key
OPENAI_API_KEY=your_openai_api_key

# Contract addresses (will be filled after deployment)
REPUTATION_ORACLE_ADDRESS=
REPUTATION_PASSPORT_ADDRESS=
REPUTATION_GATE_ADDRESS=
CROSS_CHAIN_REPUTATION_ADDRESS=

# Frontend environment variables
NEXT_PUBLIC_REPUTATION_ORACLE_ADDRESS=
NEXT_PUBLIC_REPUTATION_PASSPORT_ADDRESS=
NEXT_PUBLIC_REPUTATION_GATE_ADDRESS=
NEXT_PUBLIC_CROSS_CHAIN_REPUTATION_ADDRESS=

# The Graph API (optional - for enhanced queries)
THE_GRAPH_API_KEY=your_the_graph_api_key

# 0G AI API (for enhanced AI explanations)
ZERO_G_API_KEY=your_zero_g_api_key

# Hypergraph API (for metadata publishing)
HYPERGRAPH_API_KEY=your_hypergraph_api_key
